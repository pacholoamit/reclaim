{
  "name": "reclaim",
  "version": "1.0.0",
  "description": "A CLI tool to recursively delete cache folders for developers",
  "module": "index.ts",
  "type": "module",
  "bin": {
    "reclaim": "./index.ts"
  },
  "scripts": {
    "start": "bun run index.ts",
    "dev": "bun run --watch index.ts",
    "build": "bun build index.ts --outdir ./dist --target bun",
    "install-global": "bun link"
  },
  "keywords": ["cli", "cache", "cleanup", "developer-tools", "nodejs", "bun"],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@clack/core": "^0.5.0",
    "@clack/prompts": "^0.11.0"
  }
}
